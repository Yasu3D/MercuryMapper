                                        <Border Width="20" Height="20" CornerRadius="3" Background="White" Name="TraceColorBlue"/>
                                        <TextBlock Margin="4,0,0,0" VerticalAlignment="Center" Text="{x:Static lang:Resources.Editor_Color_TraceBlue}"/>
                                    </StackPanel>
                                </ComboBoxItem>
                                <ComboBoxItem>
                                    <StackPanel Orientation="Horizontal">
                                        <Border Width="20" Height="20" CornerRadius="3" Background="White" Name="TraceColorViolet"/>
                                        <TextBlock Margin="4,0,0,0" VerticalAlignment="Center" Text="{x:Static lang:Resources.Editor_Color_TraceViolet}"/>
                                    </StackPanel>
                                </ComboBoxItem>
                                <ComboBoxItem>
                                    <StackPanel Orientation="Horizontal">
                                        <Border Width="20" Height="20" CornerRadius="3" Background="White" Name="TraceColorPink"/>
                                        <TextBlock Margin="4,0,0,0" VerticalAlignment="Center" Text="{x:Static lang:Resources.Editor_Color_TracePink}"/>
                                    </StackPanel>
                                </ComboBoxItem>
                            </ComboBox>
                        </UniformGrid>
                        
                        <Button Margin="0,0,0,4" HorizontalAlignment="Stretch" Click="ButtonPaintTraces_OnClick" Content="{x:Static lang:Resources.Editor_PaintTraces}" ToolTip.Tip="{x:Static lang:Resources.ToolTip_PaintTraces}"/>
                        
                        <Separator/>
                        
                        <UniformGrid Columns="2" Margin="0,4,0,4">
                            <TextBlock Text="{x:Static lang:Resources.Editor_ScrollLayer}" VerticalAlignment="Center" Margin="4,0,0,0"/>
                            <ComboBox Name="ScrollLayerComboBox" HorizontalAlignment="Stretch" Margin="0,0,2,0" SelectedIndex="0">
                                <ComboBoxItem Content="Layer 0"/>
                                <ComboBoxItem Content="Layer 1"/>
                                <ComboBoxItem Content="Layer 2"/>
                                <ComboBoxItem Content="Layer 3"/>
                                <ComboBoxItem Content="Layer 4"/>
                                <ComboBoxItem Content="Layer 5"/>
                                <ComboBoxItem Content="Layer 6"/>
                                <ComboBoxItem Content="Layer 7"/>
                                <ComboBoxItem Content="Layer 8"/>
                                <ComboBoxItem Content="Layer 9"/>
                            </ComboBox>
                        </UniformGrid>
                        
                        <Button Margin="0,0,0,4" HorizontalAlignment="Stretch" Click="ButtonSetScrollLayer_OnClick" Content="{x:Static lang:Resources.Editor_SetScrollLayer}" ToolTip.Tip="{x:Static lang:Resources.ToolTip_SetScrollLayer}"/>
                        
                        <Separator/>
                        
                        <UniformGrid Columns="2" Margin="0,4,0,4">
                            <TextBlock Text="{x:Static lang:Resources.Editor_EaseHeader}" VerticalAlignment="Center" Margin="4,0,0,0"/>
                            <ComboBox Name="HoldEaseComboBox" HorizontalAlignment="Stretch" Margin="0,0,2,0" SelectedIndex="0">
                                <ComboBoxItem Content="{x:Static lang:Resources.Editor_Ease_Linear}"/>
                                <ComboBoxItem Content="{x:Static lang:Resources.Editor_Ease_LinearPlusIn}"/>
                                <ComboBoxItem Content="{x:Static lang:Resources.Editor_Ease_LinearPlusOut}"/>
                                <ComboBoxItem Content="{x:Static lang:Resources.Editor_Ease_SmoothIn}"/>
                                <ComboBoxItem Content="{x:Static lang:Resources.Editor_Ease_SmoothOut}"/>
                                <ComboBoxItem Content="{x:Static lang:Resources.Editor_Ease_SharpIn}"/>
                                <ComboBoxItem Content="{x:Static lang:Resources.Editor_Ease_SharpOut}"/>
                                <ComboBoxItem Content="{x:Static lang:Resources.Editor_Ease_SineIn}"/>
                                <ComboBoxItem Content="{x:Static lang:Resources.Editor_Ease_SineOut}"/>
                                <ComboBoxItem Content="{x:Static lang:Resources.Editor_Ease_QuadIn}"/>
                                <ComboBoxItem Content="{x:Static lang:Resources.Editor_Ease_QuadOut}"/>
                            </ComboBox>
                        </UniformGrid>
                        
                        <Button HorizontalAlignment="Stretch" Margin="0,0,0,4" Click="ButtonBakeHold_OnClick" Content="{x:Static lang:Resources.Editor_BakeNotes}" ToolTip.Tip="{x:Static lang:Resources.ToolTip_BakeNoteCollection}"/>
                        <Button HorizontalAlignment="Stretch" Margin="0,0,0,4" Click="ButtonInsertHoldSegment_OnClick" Content="{x:Static lang:Resources.Editor_InsertSegment}" ToolTip.Tip="{x:Static lang:Resources.ToolTip_InsertSegment}"/>
                        
                        <Separator/>
                        
                        <Button HorizontalAlignment="Stretch" Margin="0,4,0,4" Click="ButtonStitchHold_OnClick" Content="{x:Static lang:Resources.Editor_StitchNoteCollection}" ToolTip.Tip="{x:Static lang:Resources.ToolTip_StitchCollection}"/>
                        <Button HorizontalAlignment="Stretch" Margin="0,0,0,4" Click="ButtonSplitHold_OnClick" Content="{x:Static lang:Resources.Editor_SplitNoteCollection}" ToolTip.Tip="{x:Static lang:Resources.ToolTip_SplitNoteCollection}"/>

                        <Separator/>
                        
                        <Button HorizontalAlignment="Stretch" Margin="0,4,0,4" Click="ButtonSetRenderTrue_OnClick" Content="{x:Static lang:Resources.Editor_SetRenderTrue}" ToolTip.Tip="{x:Static lang:Resources.ToolTip_SetRenderTrue}"/>
                        <Button HorizontalAlignment="Stretch" Margin="0,0,0,4" Click="ButtonSetRenderFalse_OnClick" Content="{x:Static lang:Resources.Editor_SetRenderFalse}" ToolTip.Tip="{x:Static lang:Resources.ToolTip_SetRenderFalse}"/>
                        
                        <Separator/>

                        <Button HorizontalAlignment="Stretch" Margin="0,4,0,4" Click="ButtonDeleteSegments_OnClick" Content="{x:Static lang:Resources.Editor_DeleteSegments}" ToolTip.Tip="{x:Static lang:Resources.ToolTip_DeleteSegments}" Foreground="Red"/>
                    </StackPanel>
                </Expander>
                
                <!-- Modify Gimmicks -->
                <Expander Header="{x:Static lang:Resources.Editor_ExpanderHeader_ModifyGimmicks}">
                    <StackPanel>
                        <Button HorizontalAlignment="Stretch" Margin="0,0,0,4" Click="ButtonEditGimmick_OnClick" Content="{x:Static lang:Resources.Editor_EditGimmick}"/>
                        <Button HorizontalAlignment="Stretch" Margin="0,0,0,0" Click="ButtonDeleteGimmick_OnClick" Content="{x:Static lang:Resources.Editor_DeleteGimmick}" Foreground="Red"/>
                    </StackPanel>
                </Expander>
                
                <!-- Miscellaneous -->
                <Expander Header="{x:Static lang:Resources.Editor_ExpanderHeader_Miscellaneous}">
                    <StackPanel>
                        <Button HorizontalAlignment="Stretch" Margin="0,0,0,4" Click="ButtonAddComment_OnClick" Content="{x:Static lang:Resources.Editor_AddComment}"/>
                        
                        <Separator/>
                        
                        <UniformGrid Columns="2" Margin="0,4,0,0">
                            <Button HorizontalAlignment="Stretch" Margin="0,0,2,0" Click="ButtonSetLoopStart_OnClick" Content="{x:Static lang:Resources.Editor_SetLoopStart}"/>
                            <Button HorizontalAlignment="Stretch" Margin="2,0,0,0" Click="ButtonSetLoopEnd_OnClick" Content="{x:Static lang:Resources.Editor_SetLoopEnd}"/>
                        </UniformGrid>
                    </StackPanel>
                </Expander>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
